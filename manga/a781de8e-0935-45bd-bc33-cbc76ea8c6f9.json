  "title": {
   "en": "Ore ga Watashi ni Naru made"
  },
  "altTitles": [
   {
    "en": "Until I Become Me"
   },
   {
    "ja": "オレが私になるまで"
   },
   {
    "ja-ro": "Orewata"
   },
   {
    "pt-br": "Até Que Eu (Ele) Me Torne Ela"
   }
